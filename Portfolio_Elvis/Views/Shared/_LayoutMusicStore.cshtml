<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/css/msStyle.css")" rel="stylesheet"
          type="text/css" />
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />*@

    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")"
            type="text/javascript"></script>
</head>
<body>
    <div id="header">
        <h1><a href="/">ASP.NET MVC MUSIC STORE</a></h1>
        <ul id="navlist">
            <li class="first"><a href="@Url.Content("~")" id="current">Home</a></li>
            <li><a href="@Url.Content("~/MusicStore/")">Store</a></li>
            @*<li>@{Html.RenderAction("CartSummary", "ShoppingCart");}</li>*@
            <li><a href="@Url.Content("~/MusicStoreManager/")">Admin</a></li>
        </ul>
        <partial name="~/Views/Shared/_LoginPartial.cshtml" />

    </div>

    @*@{Html.RenderAction("GenreMenu", "Store");}*@

    <div id="main">
        @RenderBody()
    </div>

    <div id="footer">
        built with <a href="http://asp.net/mvc">ASP.NET MVC</a>
    </div>

    @RenderSection("Scripts", required: false)

</body>
</html>